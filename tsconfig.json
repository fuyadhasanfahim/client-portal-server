{
    "compilerOptions": {
        "module": "esnext",
        "target": "ES2021",
        "lib": ["ES2021", "DOM"],
        "rootDir": "./src",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": ["node", "express"],
        "moduleResolution": "node"
    }
}
